//CTreeNode
function CTreeNode(pTree, pParent, stName, stURL)
{
	this.pTree =3D pTree;
	this.pParent =3D pParent;
	this.stName =3D stName;
	this.stURL =3D stURL;
	this.bOpen =3D false;
	this.bFolder =3D true;
	this.nArrIndex =3D 0;
	this.Children =3D new Array;

	if (pParent)
		pParent.AddChild(this);
}

CTreeNode.prototype.AddChild =3D function(pNode)
{
	this.bFolder =3D true;
	this.Children[this.Children.length] =3D pNode;
}

CTreeNode.prototype.GetChildrenHTMLCode =3D function(nLevel)
{
	var st =3D '';

	for (var i=3D0; i<this.Children.length; i++)
		st +=3D this.Children[i].GetHTMLCode(nLevel);

	return st;
}

CTreeNode.prototype.GetHTMLCode =3D function(nLevel)
{
	var st =3D '<div><table border=3D"0" cellspacing=3D"0" =
cellpadding=3D"0">';
	st +=3D '<tr><td valign=3D"middle" nowrap>';

	//Shift spaces
	for (var i=3D0; i<nLevel; i++)
		st +=3D '<img src=3D"' + this.pTree.Files.imgEmpty + '" ' + =
this.pTree.Files.stIconProps + '>';

	//Sign or extra space
	if (this.bFolder)
	{
		st +=3D '<a href=3D"javascript:void(0)" onclick=3D"javascript: ' + =
this.pTree.stName + '.OnNodeClick(' + this.nArrIndex + ',true);">';
		st +=3D '<img id=3D"sign' + this.pTree.stName + this.nArrIndex + '" =
src=3D"';
		st +=3D this.bOpen ? this.pTree.Files.imgMinus : =
this.pTree.Files.imgPlus;
		st +=3D '" ' + this.pTree.Files.stIconProps + '></a>';
	}
	else
		st +=3D '<img src=3D"' + this.pTree.Files.imgEmpty + '" ' + =
this.pTree.Files.stIconProps + '>';

	//General link code
	var stA =3D '<a href=3D"' + this.stURL + '" class=3D"listtree"';
	if (this.pTree.stTarget)
		stA +=3D ' target=3D"' + this.pTree.stTarget + '"';
	if (this.bFolder)
		stA +=3D ' onclick=3D"javascript: ' + this.pTree.stName + =
'.OnNodeClick(' + this.nArrIndex + ',false);"';
	stA +=3D'>';

	//Icon
	if (this.pTree.bShowIcons)
	{
		st +=3D stA;
		st +=3D '<img id=3D"icon' + this.pTree.stName + this.nArrIndex + '" =
src=3D"';
		st +=3D (!this.bFolder) ? this.pTree.Files.imgNode : (this.bOpen ? =
this.pTree.Files.imgOpenFolder : this.pTree.Files.imgClosedFolder);
		st +=3D '" ' + this.pTree.Files.stIconProps + '>';
		st +=3D "</a>";
	}

        st +=3D '</td><td valign=3D"middle" nowrap>';

	//Text
	st +=3D stA;
	st +=3D this.stName;
	st +=3D '</a>'

	st +=3D '</td></tr></table></div>';

	//Children
	if (this.bFolder)
	{
		st +=3D '<div id=3D"div' + this.pTree.stName + this.nArrIndex + '" =
style=3D"display:' + (this.bOpen ? 'block' : 'none') + ';">';
		st +=3D this.GetChildrenHTMLCode(nLevel+1);
		st +=3D '</div>';
	}

	return st;
}

CTreeNode.prototype.SetOpen =3D function(bOpen)
{
	document.getElementById('sign' + this.pTree.stName + =
this.nArrIndex).src =3D bOpen ? this.pTree.Files.imgMinus : =
this.pTree.Files.imgPlus;
	if (this.pTree.bShowIcons)
		document.getElementById('icon' + this.pTree.stName + =
this.nArrIndex).src =3D
			(!this.bFolder) ? this.pTree.Files.imgNode : (bOpen ? =
this.pTree.Files.imgOpenFolder : this.pTree.Files.imgClosedFolder);
	document.getElementById('div' + this.pTree.stName + =
this.nArrIndex).style.display =3D bOpen ? 'block' : 'none';

	this.bOpen =3D bOpen;
